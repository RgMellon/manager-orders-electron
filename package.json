{
  "name": "UaufiDesktop",
  "license": "MIT",
  "version": "0.1.0",
  "author": "Uau-fi Inc",
  "description": "Uaufi order delivery Desktop",
  "private": true,
  "main": "public/electron.js",
  "build": {
    "appId": "teste.app",
    "mac": {
      "icon": "public/icons/mac/uau.icns",
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "./public/icons/win/uau.png"
    },
    "files": [
      "./build/**/*",
      "./dist/**/*",
      "./node_modules/**/*",
      "./public/**/*",
      "*.js"
    ],
    "directories": {
      "buildResources": "public"
    }
  },
  "homepage": "./",
  "dependencies": {
    "@react-native-community/async-storage": "^1.8.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@unform/core": "^2.0.0",
    "@unform/web": "^2.0.0",
    "axios": "^0.19.2",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.1.0",
    "history": "^4.10.1",
    "immer": "^5.0.0",
    "polished": "^3.4.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-icons": "^3.9.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-to-print": "^2.6.3",
    "react-toastify": "^5.5.0",
    "reactotron-react-js": "^3.3.7",
    "reactotron-redux": "^3.1.2",
    "reactotron-redux-saga": "^4.2.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.0.1"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "build": "npm run react-build && npm run electron-build",
    "build32": "npm run react-build && npm run electron-build -- --ia32",
    "win": "npm run react-build && npm run electron-build --windows nsis:ia32",
    "setup": "electron-installer-windows --src dist/app-win32-x64/ --dest dist/installers/",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000/ && npm run electron-dev\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "electron": "^9.4.0",
    "electron-builder": "^22.4.0",
    "electron-installer-windows": "^3.0.0",
    "wait-on": "^4.0.1"
  }
}
